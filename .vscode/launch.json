{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch FFD",
      "type": "firefox",
      "request": "launch",
      "reAttach": true,
      "url": "http://localhost:5173",
      "webRoot": "${workspaceFolder}",
      "preLaunchTask": "bunny",
      "keepProfileChanges": true,
      "profileDir": "${workspaceFolder}/.vscode/firefox",
      "skipFiles": ["<node_internals>/**", "<node_internals>/**/*.js"],
      "liftAccessorsFromPrototypes": 3,
      "log": {
        "consoleLevel": {
          "PathConversion": "Warn",
          "default": "Info"
        }
      }
    }
  ]
}
